        {#            {% include "../blocks/header-eco/header-eco.html" %}#}
        {% endif %}
        {% include "../blocks/_modules/header/header.html" %}

        {% block content %}{% endblock %}

{#        <script src="/blocks/_utilite/jquery-3.5.1.min.js"></script>#}

        {% if GsapUse == 'true' %}
            <script src="../../libs/gsap/gsap.min.js"></script>
            <script src="../../libs/gsap/ScrollTrigger.min.js"></script>
            <script>
                gsap.registerPlugin(ScrollTrigger);
            </script>
        {% endif %}

    </body>
</html>
